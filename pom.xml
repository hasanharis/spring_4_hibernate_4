<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 
http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.umarashfaq</groupId>
	<artifactId>tyrion</artifactId>
	<packaging>war</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>demo Maven Webapp</name>
	<url>http://maven.apache.org</url>

	<properties>

		<!-- START DEPENDENCY PROPERTIES -->
		<org.springframework.version>4.0.2.RELEASE</org.springframework.version>
		<org.springframework.security>3.2.1.RELEASE</org.springframework.security>
		<org.hibernate>4.3.4.Final</org.hibernate>
		<org.slf4j>1.7.6</org.slf4j>
		<!-- END DEPENDENCY PROPERTIES -->


	</properties>

	<dependencies>

		<dependency>
			<groupId>mysql</groupId>
